<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid Background="#b59ce6"></Grid>
        <Image Source="Resources/7-75302_clip-art-transparent-library-world-png-jokingart-com.png" HorizontalAlignment="Left" Margin="5"></Image>
        <TextBlock Text="Tour" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Grid Grid.Row="2" Background="#7019c2"></Grid>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrameRender" Grid.Row="1" Name="MainFrame"></Frame>
        <Button Content="Back" Name="BtnBack" Width="175" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="5" Click="BtnBack_Click"></Button>
    </Grid>
</Window>
